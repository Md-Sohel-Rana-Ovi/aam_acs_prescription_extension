<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_store_requisition" model="ir.sequence">
            <field name="name">Store Requisition</field>
            <field name="code">store.requisition</field>
            <field name="prefix">REQ/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="store_requisition_location" model="stock.location" forcecreate="1">
            <field name="name">Store Requisition Location</field>
            <field name="location_id" ref="stock.stock_location_customers"/>
            <field name="usage">customer</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
